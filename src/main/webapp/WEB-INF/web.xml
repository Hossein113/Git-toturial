<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>maktab74.servlet.LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>maktab74.servlet.SignUpServlet</servlet-class>
    </servlet>


    <servlet>
        <servlet-name>searchticket</servlet-name>
        <servlet-class>maktab74.servlet.SearcTicket</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>submitTicket</servlet-name>
        <servlet-class>maktab74.servlet.SubmitTicket</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>buy</servlet-name>
        <servlet-class>maktab74.servlet.BuyTicket</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>showTicketBuy</servlet-name>
        <servlet-class>maktab74.servlet.ShowBuyTicket</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>showinfo</servlet-name>
        <servlet-class>maktab74.servlet.ShowInfo</servlet-class>
    </servlet>


    <servlet>
        <servlet-name>deletTicket</servlet-name>
        <servlet-class>maktab74.servlet.DeletTicket</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>

    deletTicket


    <servlet-mapping>
        <servlet-name>buy</servlet-name>
        <url-pattern>/buy</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>searchticket</servlet-name>
        <url-pattern>/searchticket</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>submitTicket</servlet-name>
        <url-pattern>/submit-ticket</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>showTicketBuy</servlet-name>
        <url-pattern>/showTicketBuy</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>showinfo</servlet-name>
        <url-pattern>/showinfo</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>deletTicket</servlet-name>
        <url-pattern>/deletTicket</url-pattern>
    </servlet-mapping>

</web-app>
